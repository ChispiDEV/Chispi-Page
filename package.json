{
  "name": "chispi-page",
  "version": "1.0.0",
  "description": "Portfolio personal de Chispi",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:css && npm run build:js && jekyll serve --livereload",
    "build": "npm run build:css && npm run build:js && jekyll build",
    "build:css": "node build-scripts.js --css",
    "build:js": "node build-scripts.js --js",
    "build:all": "node build-scripts.js --all",
    "watch:css": "node build-scripts.js --watch-css",
    "watch:js": "node build-scripts.js --watch-js",
    "production": "npm run build:all && JEKYLL_ENV=production jekyll build"
  },
  "devDependencies": {
    "sass": "^1.66.1",
    "chokidar": "^3.5.3",
    "clean-css": "^5.3.2",
    "uglify-js": "^3.17.4"
  },
  "keywords": ["jekyll", "portfolio", "static-site"],
  "author": "Chispi",
  "license": "MIT"
}